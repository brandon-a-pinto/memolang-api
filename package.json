{
  "name": "api",
  "version": "0.0.1",
  "author": "Brandon Pinto",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "start": "yarn build && node dist/main/server.js",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9000 --nolazy ./dist/main/server.js",
    "up": "yarn build && docker-compose up -d",
    "down": "docker-compose down",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage",
    "prepare": "husky install",
    "prepare:linux": "yarn prepare && chmod ug+x .husky/*"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "git-commit-msg-linter": "^4.1.2",
    "husky": "^7.0.4",
    "jest": "^27.0.0",
    "jest-environment-node": "27.x.x",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {}
}
